package com.didongbabong.pojo;

import java.io.Serializable;

// This should be the general interface for all the fruit in our database
public class Food implements Serializable {
	int id;
	String name;
	
	float sugar;
	float carbon;
	float vitamin_c;
	float protein;
	float calories;
	float fat;
	float calcium;
	float sodium;
	float iron;
	float potassium;
	float copper;
	float zinc;
	float vitamin_b_6;
	float vitamin_b_12;
	float fiber;
	float cholesterol;
	
	
	
	

}
